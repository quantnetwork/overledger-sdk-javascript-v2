  "types": "dist/index.d.ts",
  "dependencies": {
    "@quantnetwork/overledger-types": "^2.4.1",
    "axios": "^1.3.4",
    "log4js": "^6.9.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "jest": "^29.5.0",
    "lerna": "^6.6.1",
    "nodemon": "^2.0.22",
    "ts-jest": "^29.0.5",
    "eslint": "^8.36.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "typescript": "^4.9.5"
